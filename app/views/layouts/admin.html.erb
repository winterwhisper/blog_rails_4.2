<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title>AdminLTE 2 | Dashboard</title>
  <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag    'admin', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'admin', 'data-turbolinks-track' => true %>
  <!--[if lt IE 9]>
  <%= javascript_include_tag 'legend', 'data-turbolinks-track' => true %>
  <![endif]-->
</head>
<body class="skin-black-light sidebar-mini">
  <div class="wrapper">
    <%= render 'layouts/admin/header' %>
    <%= render 'layouts/admin/sidebar' %>
    <div class="content-wrapper">
      <section class="content-header">
        <h1>
          <% if content_for?(:page_title) %>
            <%= yield :page_title %>
            <% if content_for?(:page_desc) %>
              <small><%= yield :page_desc %></small>
            <% end %>
          <% end %>
        </h1>
        <% if content_for?(:breadcrumb) %>
          <ol class="breadcrumb"><%= yield :breadcrumb %></ol>
        <% end %>
      </section>
      <section class="content">
        <div class="box">
          <div class="box-header with-border">
            <% if content_for?(:box_title) %>
              <h3 class="box-title"><%= yield :box_title %></h3>
            <% end %>
            <% if content_for?(:box_actions) %>
              <div class="box-tools pull-right"><%= yield :box_actions %></div>
            <% end %>
          </div>
          <div class="box-body">
            <div class="container-fluid">
              <div class="row">
                <%= render 'layouts/admin/flash_message' %>
                <%= yield %>
              </div>
            </div>
          </div>
          <% if content_for?(:box_footer) %>
            <div class="box-footer"><%= yield :box_footer %></div>
          <% end %>
        </div>
      </section>
    </div>
    <%= render 'layouts/admin/footer' %>
  </div>
</body>
</html>